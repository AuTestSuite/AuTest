Test.Summary="Test we error correctly on an Exclude pattern"

tr=Test.AddTestRun()
tr.Processes.Default.Command='echo "Error! there is a problem"'
tr.Processes.Default.ReturnCode = Testers.LessThan(10)
tr.Processes.Default.Streams.stdout=Testers.ExcludesExpression("Error!","Test should not contain errors")

